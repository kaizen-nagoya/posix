%%BeginDocument: ./nist_cert.ps
%%DocumentFonts: Helvetica-Bold, Helvetica
%%Title: NIST overhead header
%%Creator: Bob Bagwill
%%CreationDate: June 3, 1992
%%Pages: 1
%%BoundingBox: 0 -3 369 34
%%EndComments
%-------------------- Variables --------------------
/inch		{72 mul}		def
%-------------------- Fonts --------------------
/Font	/Helvetica-Bold findfont 10 scalefont def
%-------------------- Data --------------------
/institute 	(National Institute of Standards and Technology)def
/lab 		(Computer Systems Laboratory) 		def
/group 		(NIST POSIX Certification Authority) 		def
%------------ Procedures ------------ 
/doLogo {				% print NIST logo
	%-------------- Variables ------------
	%-- set unit size --
	/unit 18 def

	%-- define line width --
	/lw unit 2 mul 8 div def

	lw 2 div dup translate

	%-- define fractions of unit --
	/short unit lw 2 div sub def
	/shortshort unit lw sub def
	/radius unit 4 div lw 8 div sub def

	/rounded 1 def
	rounded setlinejoin
	lw setlinewidth

	% N
	/fr lw lw mul 2 div sqrt def
	/n_short unit fr sub def
	2 setlinecap
	0 0 moveto

	1 unit lw 2 div sub unit div scale
	0 n_short lineto
	0 unit lw 2 div unit
	lw 2 div fr 2 div add unit fr 2 div sub
	curveto
	lw 2 div fr 2 div add n_short add fr 2 div lineto
	unit fr 2 div add 0 unit lw add 0
	unit lw add fr
	curveto
	unit lw add unit lineto
	currentpoint
	%0 0 1 setrgbcolor
	stroke
	1 unit lw 2 div add unit div scale
	0 setlinecap

	% I
	moveto
	lw 1.5 mul 0 rmoveto
	0 unit neg rlineto

	% S
	unit lw add 0 rlineto
	currentpoint radius add radius -90 90 arc
	unit radius lw 2 div add sub neg 0 rlineto
	currentpoint radius add radius -90 90 arcn

	% T
	unit 1.5 mul lw add 0 rlineto
	unit .5 mul neg 0 rmoveto
	0 unit neg rlineto

	%0 1 0 setrgbcolor
	stroke
	%1 1 1 setrgbcolor
} def

	% Draw a square of size of 1 point 
/doSquare {
	0 0 moveto
	0 1 lineto
	1 1 lineto
	1 0 lineto
	closepath
} def

/doLines {
	0 0 moveto
	0 setlinecap
	3 setlinewidth
	450 0 rlineto
	0 -504 rlineto
	-450 0 rlineto
	0 504 rlineto
	stroke
	%
	%
	%	Setup for thick square line
	%0.25 inch setlinewidth
	%0 setlinecap
	%0.0 inch 0.0 inch moveto
	%0.25 inch 0 inch rlineto
	%1.75 inch 0 inch moveto
	%0 inch 0.5 inch rlineto
	%currentpoint
	%-1.75 inch 0 inch rlineto
	%moveto
	%3.5 inch 0 inch rlineto
	%stroke
} def

/doText {
	Font setfont
	2 inch 24 moveto currentpoint
	institute show
	moveto 0 -12 rmoveto currentpoint
	lab show
	moveto 0 -12 rmoveto currentpoint
	group show
} def

/doHead {
	0 0 moveto
	0 setgray
	gsave
	1.5 1.5 scale
	doLogo
	grestore
	gsave
	doText
	%		Generate filled-in boxes
	grestore
	-36 +26 translate
	%		Coordinates set to bottom-left of top left square
	gsave
	18 18 scale
	doSquare fill
	24 0 translate
	doSquare fill
	0 -27 translate
	doSquare fill
	-24 0 translate
	doSquare fill
	0 27 translate
	doSquare fill
	%		Generate border lines
	grestore
	0 +18 translate
	%		Coordinates set to top-left of top left square
	gsave
	1 1 scale
	doLines
	grestore
} def

doHead
